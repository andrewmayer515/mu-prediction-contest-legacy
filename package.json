{
  "name": "mu-prediction-contest",
  "version": "1.0.0",
  "description": "Web crawler for muscoop",
  "main": "src",
  "scripts": {
    "start": "npm run build && node lib",
    "start:login": "npm start -- login",
    "debug": "npm start -- debug",
    "debug:login": "npm start -- debug login",
    "eslint": "./node_modules/.bin/eslint --quiet src/**",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage --coverageReporters=text",
    "build": "babel src -d lib",
    "totals": "node scripts/totals",
    "post": "node scripts/post",
    "server": "nodemon  --inspect --exec babel-node src/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewmayer515/mu-prediction-contest.git"
  },
  "author": "Andrew Mayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewmayer515/mu-prediction-contest/issues"
  },
  "homepage": "https://github.com/andrewmayer515/mu-prediction-contest#readme",
  "dependencies": {
    "fast-levenshtein": "^2.0.6",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-router": "^7.4.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.times": "^4.3.2",
    "ora": "^3.0.0",
    "prompts": "^1.1.0",
    "puppeteer": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.5.0",
    "jest-puppeteer": "^3.4.0",
    "nodemon": "^1.18.10",
    "prettier": "1.16.0",
    "regenerator-runtime": "^0.12.0"
  }
